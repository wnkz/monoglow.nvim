  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#a0a0a0",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#555555"
      }
    },
    {
      "scope": [
        "constant.language",
        "support.constant",
        "variable.language.this",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#707070"
      }
    },
    {
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#555555"
      }
    },
    {
      "scope": [
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#555555"
      }
    },
    {
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#555555"
      }
    },
    {
      "scope": [
        "string.regexp",
        "constant.character.escape",
        "constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#506070"
      }
    },
    {
      "scope": [
        "keyword.control",
        "keyword.other",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#7d7d7d"
      }
    },
    {
      "scope": [
        "keyword.control.return",
        "keyword.control.flow.return"
      ],
      "settings": {
        "foreground": "#808080"
      }
    },
    {
      "scope": [
        "keyword.control.exception",
        "keyword.control.trycatch"
      ],
      "settings": {
        "foreground": "#878787"
      }
    },
    {
      "scope": [
        "punctuation.definition.parameters",
        "punctuation.definition.arguments",
        "punctuation.definition.array",
        "punctuation.definition.bracket",
        "punctuation.section.brackets",
        "punctuation.section.parens",
        "punctuation.parenthesis",
        "punctuation.brackets",
        "meta.brace.round",
        "meta.brace.square"
      ],
      "settings": {
        "foreground": "#3a3a3a"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "keyword.operator.comparison",
        "keyword.operator.arithmetic",
        "keyword.operator.logical",
        "keyword.operator.assignment",
        "keyword.operator.bitwise",
        "punctuation.separator.pointer-access"
      ],
      "settings": {
        "foreground": "#059669"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#3a3a3a"
      }
    },
    {
      "scope": [
        "meta.function-call",
        "meta.function-call.generic",
        "support.function.builtin"
      ],
      "settings": {
        "foreground": "#707070"
      }
    },
    {
      "scope": [
        "variable.parameter",
        "meta.parameters",
        "meta.function.parameters"
      ],
      "settings": {
        "foreground": "#767676"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#555555"
      }
    },
    {
      "scope": [
        "entity.name.type.primitive",
        "storage.type.primitive",
        "storage.type.built-in",
        "storage.type.numeric",
        "support.type.primitive"
      ],
      "settings": {
        "foreground": "#555555"
      }
    },
    {
      "scope": [
        "entity.other.inherited-class",
        "storage.type.class"
      ],
      "settings": {
        "foreground": "#3a3a3a"
      }
    },
    {
      "scope": [
        "variable",
        "variable.other",
        "variable.language"
      ],
      "settings": {
        "foreground": "#3a3a3a"
      }
    },
    {
      "scope": [
        "variable.other.property",
        "variable.other.member",
        "support.variable.property",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#747474"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "support.class.component"
      ],
      "settings": {
        "foreground": "#555555"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#747474"
      }
    },
    {
      "scope": [
        "punctuation.definition.tag",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.section"
      ],
      "settings": {
        "foreground": "#3a3a3a"
      }
    },
    {
      "scope": [
        "punctuation.definition.string",
        "punctuation.definition.parameters",
        "punctuation.definition.array",
        "punctuation.definition.bracket",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#3a3a3a"
      }
    },
    {
      "scope": [
        "markup.heading"
      ],
      "settings": {
        "foreground": "#707070",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.strikethrough"
      ],
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": [
        "markup.underline"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#a0a0a0",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#555555"
      }
    },
    {
      "scope": [
        "markup.list"
      ],
      "settings": {
        "foreground": "#059669"
      }
    }
  ]
