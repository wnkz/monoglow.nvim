  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#585858",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#b1b1b1"
      }
    },
    {
      "scope": [
        "constant.language",
        "support.constant",
        "variable.language.this",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#a1a1a1"
      }
    },
    {
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#b1b1b1"
      }
    },
    {
      "scope": [
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#b1b1b1"
      }
    },
    {
      "scope": [
