  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#585858",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#aaaaaa"
      }
    },
    {
      "scope": [
        "constant.language",
        "support.constant",
        "variable.language.this",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#7a7a7a"
      }
    },
    {
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#aaaaaa"
      }
    },
    {
      "scope": [
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#aaaaaa"
      }
    },
    {
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#aaaaaa"
      }
    },
    {
      "scope": [
        "string.regexp",
        "constant.character.escape",
        "constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#708090"
      }
    },
    {
      "scope": [
        "keyword.control",
        "keyword.other",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#707070"
      }
    },
    {
      "scope": [
        "keyword.control.return",
        "keyword.control.flow.return"
      ],
      "settings": {
        "foreground": "#6c6c6c"
      }
    },
    {
      "scope": [
        "keyword.control.exception",
        "keyword.control.trycatch"
      ],
      "settings": {
        "foreground": "#616161"
      }
    },
    {
      "scope": [
        "punctuation.definition.parameters",
        "punctuation.definition.arguments",
        "punctuation.definition.array",
        "punctuation.definition.bracket",
        "punctuation.section.brackets",
        "punctuation.section.parens",
        "punctuation.parenthesis",
        "punctuation.brackets",
        "meta.brace.round",
        "meta.brace.square"
      ],
      "settings": {
        "foreground": "#cccccc"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "keyword.operator.comparison",
        "keyword.operator.arithmetic",
        "keyword.operator.logical",
        "keyword.operator.assignment",
        "keyword.operator.bitwise",
        "punctuation.separator.pointer-access"
      ],
      "settings": {
        "foreground": "#1bfd9c"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#cccccc"
      }
    },
    {
      "scope": [
        "meta.function-call",
        "meta.function-call.generic",
        "support.function.builtin"
      ],
      "settings": {
        "foreground": "#7a7a7a"
      }
    },
    {
      "scope": [
        "variable.parameter",
        "meta.parameters",
        "meta.function.parameters"
      ],
      "settings": {
        "foreground": "#797979"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#aaaaaa"
      }
    },
    {
      "scope": [
        "entity.name.type.primitive",
        "storage.type.primitive",
        "storage.type.built-in",
        "storage.type.numeric",
        "support.type.primitive"
      ],
      "settings": {
        "foreground": "#aaaaaa"
      }
    },
    {
      "scope": [
        "entity.other.inherited-class",
        "storage.type.class"
      ],
      "settings": {
        "foreground": "#cccccc"
      }
    },
    {
      "scope": [
        "variable",
        "variable.other",
        "variable.language"
      ],
      "settings": {
        "foreground": "#cccccc"
      }
    },
    {
      "scope": [
        "variable.other.property",
        "variable.other.member",
        "support.variable.property",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#8c8c8c"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "support.class.component"
      ],
      "settings": {
        "foreground": "#aaaaaa"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#8c8c8c"
      }
    },
    {
      "scope": [
        "punctuation.definition.tag",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.section"
      ],
      "settings": {
        "foreground": "#cccccc"
      }
    },
    {
      "scope": [
        "punctuation.definition.string",
        "punctuation.definition.parameters",
        "punctuation.definition.array",
        "punctuation.definition.bracket",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#cccccc"
      }
    },
    {
      "scope": [
        "markup.heading"
      ],
      "settings": {
        "foreground": "#7a7a7a",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.strikethrough"
      ],
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": [
        "markup.underline"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#585858",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#aaaaaa"
      }
    },
    {
      "scope": [
        "markup.list"
      ],
      "settings": {
        "foreground": "#1bfd9c"
      }
    }
  ]
}
